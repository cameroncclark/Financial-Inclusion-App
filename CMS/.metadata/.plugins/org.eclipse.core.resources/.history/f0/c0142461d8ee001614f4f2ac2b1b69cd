package ContentObjects;

public class Category {
	String name;
	int id;
	
	public Category(String name, int ID) {
		this.name = name;
		this.id = ID;
	}
	
	public Category() {
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}

}
